APP_NAME="Audio Lara"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
# Development: http://localhost:8000 (php artisan serve)
# XAMPP: http://localhost/audio-lara/public
# Production: https://yourdomain.com
APP_URL=http://localhost:8000

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=audio_lara
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_APP_NAME="${APP_NAME}"

# Chrome/Puppeteer Configuration for Crawling
# Set this to your Chrome executable path for hosting environments
# Leave empty for local development (auto-detection)
PUPPETEER_EXECUTABLE_PATH=

# For shared hosting (if Chrome is available)
# PUPPETEER_EXECUTABLE_PATH="/usr/bin/google-chrome"

# For VPS/Cloud hosting
# PUPPETEER_EXECUTABLE_PATH="/usr/bin/google-chrome-stable"

# For Docker
# PUPPETEER_EXECUTABLE_PATH="/usr/bin/google-chrome"

# For Heroku (with buildpack)
# PUPPETEER_EXECUTABLE_PATH="/app/.apt/usr/bin/google-chrome-stable"

# Skip Chromium download (for hosting with pre-installed Chrome)
# PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# VBee TTS API Configuration
VBEE_APP_ID=your_vbee_app_id_here
VBEE_ACCESS_TOKEN=your_vbee_access_token_here

# TikTok API Configuration
TIKTOK_CLIENT_ID=your_tiktok_client_id_here
TIKTOK_CLIENT_SECRET=your_tiktok_client_secret_here
TIKTOK_REDIRECT_URI=http://localhost:8000/admin/channels/tiktok/callback
TIKTOK_SANDBOX=true
TIKTOK_API_VERSION=v2
